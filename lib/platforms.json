            "nightly": {
                "defaultArch": "arm-v15",
                "type": "classic",
                "arches": {
                    "arm-v15": {
                        "path": "mobile/nightly/latest-mozilla-central-android-api-15/",
                        "fileEnding": ".android-arm.apk"
                    },
                    "x86": {
                        "path": "mobile/nightly/latest-mozilla-central-android-x86/",
                        "fileEnding": ".android-i386.apk"
                    }
                }
            },
            "beta": {
                "defaultArch": "arm-v15",
                "type": "mdu",
                "arches": {
                    "arm-v15": {
                        "platform": "android",
                        "product": "fennec-beta-latest"
                    },
                    "x86": {
                        "platform": "android-x86",
                        "product": "fennec-beta-latest"
                    }
                }
            },
            "release": {
                "defaultArch": "arm-v15",
